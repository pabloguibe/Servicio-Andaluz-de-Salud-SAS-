<%- include("Template/cabecera", {tituloweb: "Pagina de inicio"}) %>
    <div class="container">
        <h1>Crear nueva Persona Mayor</h1>
        <br>
        <form action="/PersonaMayor" class="my-2" method="POST">
            <input type="text" placeholder="nombre" class="form-control my-2" name="nombre">
            <input type="text" placeholder="apellido1" class="form-control my-2" name="apellido1">
            <input type="text" placeholder="apellido2" class="form-control my-2" name="apellido2">
            <input type="text" placeholder="telefono" class="form-control my-2" name="telefono">
            <input type="text" placeholder="dni" class="form-control my-2" name="dni">
            <input type="text" placeholder="patologias" class="form-control my-2" name="patologias">
            <input type="text" placeholder="fechavisita" class="form-control my-2" name="fechavisita">
            <input type="text" placeholder="lugar" class="form-control my-2" name="lugar">
            <input type="text" placeholder="observacion" class="form-control my-2" name="observacion">
            <button type="submit" class="btn btn-dark btn-block">
                Agregar
            </button>
        </form>
    </div>
<%- include("Template/footer") %>